    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "electron-squirrel-startup": "^1.0.1",
    "env-var": "^7.5.0",
    "fluent-ffmpeg": "^2.1.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.9.1",
    "react-scripts": "5.0.1",
    "sass": "^1.89.2",
