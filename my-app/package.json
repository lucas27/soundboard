  "license": "MIT",
  "author": {
    "name": "Lucas"
  },
  "type": "commonjs",
  "main": "public/electron.js",
  "scripts": {
    "react-start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "electron .",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run dev\" ",
    "make": "yarn build && cross-env NODE_ENV=production electron-forge make",
    "package": "electron-forge package",
    "electron:package:win": "electron-builder -w -c.extraMetadata.main=build/electron.js"
  },
  "dependencies": {
